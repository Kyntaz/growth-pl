fizzbuzz :seed {
    value := $(# - 1);
    :? $value % 3 = 0, {
        :write "Fizz", bogus;
    };
    :? $value % 5 = 0, {
        :write "Buzz", bogus;
    };
    :? ($value % 3 != 0) & ($value % 5 != 0), {
        :write $value;
    }, {
        :write "";
    };
    := $value + 1;
    :? $value < 30, fizzbuzz, {};
};

:trunk {
    := 1;
    :branch fizzbuzz;
};
